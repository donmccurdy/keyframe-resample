{
  "name": "keyframe-resample",
  "version": "0.0.1",
  "description": "Resamples and optimizes keyframe data using WebAssembly",
  "type": "module",
  "sideEffects": false,
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "main": "./dist/keyframe-resample-node.cjs",
  "module": "./dist/keyframe-resample-browser.modern.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "node": {
      "require": "./dist/keyframe-resample-node.cjs",
      "default": "./dist/keyframe-resample-node.modern.js"
    },
    "default": {
      "require": "./dist/keyframe-resample-browser.cjs",
      "default": "./dist/keyframe-resample-browser.modern.js"
    }
  },
  "repository": "github:donmccurdy/keyframe-resample-wasm",
  "author": "Don McCurdy <dm@donmccurdy.com>",
  "license": "MIT",
  "scripts": {
    "dist": "yarn asbuild && yarn build",
    "build": "yarn build:node && yarn build:browser",
    "build:node": "microbundle build --raw --no-compress --target node --format modern,cjs --output dist/keyframe-resample-node.js --external node:fs/promises --alias ./resample_wasm.env.js=./resample_wasm.node.js",
    "build:browser": "microbundle build --raw --no-compress --target web --format modern,cjs --output dist/keyframe-resample-browser.js --external ./release.wasm --alias ./resample_wasm.env.js=./resample_wasm.browser.js",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "clean": "rimraf dist/*",
    "test": "ava test/*.ts",
    "benchmark": "node --loader=ts-node/esm benchmark/benchmark.ts",
    "coverage": "c8 --reporter=lcov --reporter=text ava test/*.ts --tap",
    "coverage:report": "c8 report --reporter=text-lcov > coverage/coverage.lcov",
    "preversion": "yarn dist && yarn test",
    "version": "rimraf dist/* && yarn dist && git add -u",
    "postversion": "git push && git push --tags && npm publish && yarn coverage:report"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "assemblyscript": "^0.27.3",
    "ava": "5.2.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "microbundle": "0.15.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "browserslist": [
    "defaults",
    "not IE 11",
    "node >= 14"
  ],
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "dependencies": {
    "c8": "^7.11.3"
  }
}
